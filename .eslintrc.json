{
  "env": {
    "jest/globals": true,
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "jest",
    "json-format"
  ],
  "ignorePatterns": [
    "/dist"
  ],
  "rules": {
    // "import/order":"off",
    // "consistent-return":"off",
    // "no-underscore-dangle":"off",
    // "camelcase":"off",
    "no-console":"off",
    // "no-undef":"off",
    // "no-use-before-define":"off",
    // "max-len":"off"
  }
}
